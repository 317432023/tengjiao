<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tengjiao.seed.admin.dao.sys.mapper.JobMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tengjiao.seed.admin.model.sys.entity.Job">
        <id column="id" property="id" />
        <result column="bean_name" property="beanName" />
        <result column="method_name" property="methodName" />
        <result column="method_params" property="methodParams" />
        <result column="cron_expression" property="cronExpression" />
        <result column="status" property="status" />
        <result column="remark" property="remark" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>


    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, bean_name, method_name, method_params, cron_expression, status, remark, create_time, update_time
    </sql>


</mapper>
